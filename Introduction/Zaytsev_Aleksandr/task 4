#include <stdio.h>
#include <locale.h>
int main()
{
    
    setlocale(LC_ALL, "Rus");

    float v1, v2, s, t, S;
    printf("Введите скорость первого авто\n");
    scanf("%f", &v1);
    printf("Введите скорость второго авто\n");
    scanf("%f", &v2);
    printf("Введите расстояние между авто\n");
    scanf("%f", &s);
    printf("Введите время движения авто\n");
    scanf("%f", &t);
    S = (v1 + v2) * t + s;
    printf("авто удалилсь на %.2f km", S);
    
}
