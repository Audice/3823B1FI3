#include <stdio.h>

int main()
{
    
    float a, b, sum, raz, proiz, chast;
    scanf("%f %f", &a, &b);
    printf("%.2f %.2f\n", a, b);
    sum = a + b;
    printf("Sum of a and b is %.3f\n", sum);
    raz = a - b;
    printf("raznost of a and b is %.3f\n", raz); 
    proiz = a * b;
    printf("proizvedenie of a and b is %.3f\n", proiz); 
    chast = a / b;
    printf("chastnoe of a and b is %.3f\n", chast); 


    
    return 0;
}
